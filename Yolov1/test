import torch

# Create a tensor with shape (2, 3, 4, 5)
tensor = torch.randn(2, 3, 4, 5)

# Flatten the tensor except the last two dimensions
flattened_tensor = torch.flatten(tensor, start_dim=0, end_dim=-2)

# Print the shape of the flattened tensor
print(flattened_tensor.shape)  # Output: torch.Size([2, 3, 20])
